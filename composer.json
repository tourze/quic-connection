{
  "name": "tourze/quic-connection",
  "description": "QUIC协议连接管理库 - 提供连接生命周期、状态机、路径管理功能",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "tourze/enum-extra": "0.1.*",
    "tourze/quic-congestion": "0.0.*",
    "tourze/quic-core": "0.0.*",
    "tourze/quic-frames": "0.0.*",
    "tourze/quic-packets": "0.0.*",
    "tourze/quic-recovery": "0.0.*",
    "tourze/quic-streams": "0.0.*",
    "tourze/quic-tls": "0.0.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\QUIC\\Connection\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\QUIC\\Connection\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}